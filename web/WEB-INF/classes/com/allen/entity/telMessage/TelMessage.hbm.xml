<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.allen.entity.telMessage">
    <class name="TelMessage" table="t_telMessage" dynamic-update="true">
        <id name ="telMessageId" column="telMessageId">
            <generator class="native"/>
        </id>
        <property name="telNum" column="telNum"/>
        <property name="countAll" column="countAll"/>
        <property name="allTime" column="allTime"/>
        <property name="countOutAll" column="countOutAll"/>
        <property name="outAllTime" column="outAllTime"/>
        <property name="count_1out" column="count_1out"/>
        <property name="_1outTime" column="_1outTime"/>
        <property name="count$1out" column="count$1out"/>
        <property name="$1outTime" column="$1outTime"/>
        <property name="count$3out" column="count$3out"/>
        <property name="$3outTime" column="$3outTime"/>
        <property name="countInAll" column="countInAll"/>
        <property name="inAllTime" column="inAllTime"/>
        <property name="count_1in" column="count_1in"/>
        <property name="_1inTime" column="_1inTime"/>
        <property name="count$1in" column="count$1in"/>
        <property name="$1inTime" column="$1inTime"/>
        <property name="count$3in" column="count$3in"/>
        <property name="$3inTime" column="$3inTime"/>
        <property name="date" column="Date"/>
        <many-to-one name="person" class="Person" column="personId"/>
    </class>

</hibernate-mapping>

     
     
     
     
     
     
     
     
     
     
     
     
     
     
     
     
     
     